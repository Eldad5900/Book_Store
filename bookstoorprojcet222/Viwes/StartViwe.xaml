<UserControl x:Class="bookstoorprojcet222.Viwes.StartViwe"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:bookstoorprojcet222.Viwes"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
              DataContext="{Binding Source={StaticResource Locator}, Path=Start}">
    <Grid Background="LightSteelBlue">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Command="{Binding BuyerCommand}" Content="Buyer" Height="50" Width="200"></Button>
        <Button Command="{Binding ManigerCommand}" Content="Manager" Height="50" Width="200" Grid.Row="1"/>
        
    </Grid>
</UserControl>
