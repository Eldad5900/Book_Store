<UserControl x:Class="bookstoorprojcet222.Viwes.MangerMinuView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
              DataContext="{Binding Source={StaticResource Locator}, Path=MinManiger}">
    <ListView Background="LightSteelBlue">
        <Button Content="Add" Command="{Binding  MyAddBook}"/>
